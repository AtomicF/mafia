create table "Table"."Messages"
(
    message_id serial  not null,
    game_id    serial  not null
        constraint messages_pk
            primary key,
    user_id    serial  not null,
    text       text    not null,
    addressee  integer not null,
    column_6   integer
);

alter table "Table"."Messages"
    owner to postgres;

create unique index messages_game_id_uindex
    on "Table"."Messages" (game_id);

create unique index messages_message_id_uindex
    on "Table"."Messages" (message_id);

